\f1\b EJERCICIO 2 PAG 104 PDF
\f0\b0 \
\
SELECT DISTINCT alumnos.nom_alum, asignaturas.nom_asig\
FROM alumnos, asignaturas, matriculas\
WHERE matriculas.dni = alumnos.dni and matriculas.cod_asig = asignaturas.cod_asig\
\
\

\f1\b EJERCICIO 3
\f0\b0 \
\
SELECT alumnos.nom_alum, matriculas.calificacion\
FROM alumnos, matriculas\
WHERE matriculas.calificacion > 4 and matriculas.cod_asig = 2 and matriculas.dni = alumnos.dni\
\

\f1\b EJERCICIO 4
\f0\b0 \
\
