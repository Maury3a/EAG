FROM alumnos, matriculas, asignaturas\
WHERE alumnos.dni = matriculas.dni AND matriculas.calificacion = '7'AND matriculas.cod_asig = asignaturas.cod_asig AND asignaturas.nom_asig = 'Matematicas';\
\

\f1\b \'95 Mostrar las aulas en las que se da clase de asignaturas que aun no tienen asignados cr\'e9ditos.\
\

\f0\b0 SELECT grupo.cod_aula\
FROM grupo, asignaturas\
WHERE grupo.cod_asig = asignaturas.cod_asig AND asignaturas.creditos = '0';\
\

\f1\b \'95 Mostrar los nombres y las \'e1reas de los profesores, que imparten clase por la ma\'f1ana\
\

